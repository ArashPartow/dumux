[TimeLoop]
DtInitial = 1000 # [s]
TEnd = 1000000 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 10 10

[Problem]
InjectionRateGas = 1e-3
InjectionRateWater = 0.0
EnableGravity = false

[PoroElastic.Problem]
Name = poroelastic # name passed to the output routines

[TwoP.Problem]
Name = twop # name passed to the output routines

[SpatialParams]
Permeability = 1e-12 # [m^2]
InitialPorosity = 0.2 # [-]

[LinearSolver]
ResidualReduction = 1e-20
MaxIterations = 2000

[Component]
SolidDensity = 2700
