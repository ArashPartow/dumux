[TimeManager]
DtInitial = 1 # [s]
TEnd = 100 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 3 1
Cells = 30 20

[Problem]
OutputName = "zeroeq2cni"
InjectionVelocity = 0.5 # [m/s]
InjectionConcentration = .1 # [-]
WallTemperature = 297.0 # [K]

[ZeroEq]
# Eddy Conductivity Models
# 0 = none
# 1 = Reynolds analogy
# 2 = modified Van Driest
# 3 = Deissler
# 4 = Meier and Rotta
EddyViscosityModel = 1
EddyDiffusivityModel = 3
EddyConductivityModel = 3
BBoxMinSandGrainRoughness = 0.1 # [m]
BBoxMaxSandGrainRoughness = 0.1 # [m]
