[TimeLoop]
DtInitial = 1 # [s]
TEnd = 1000 # [s]

[Grid]
Verbosity = true
Positions0 = 0.0 6.0
Positions1 = 0.0 0.5
Cells0 = 10
Cells1 = 20
Grading1 = 1.4

[Problem]
Name = test_channel_zeroeq2c # name passed to the output routines
InletVelocity = 0.1 # [m/s]
EnableGravity = false

[RANS]
EddyViscosityModel = 3
TurbulentSchmidtNumber = 0.7

[Assembly]
NumericDifferenceMethod = 0
NumericEpsilon.BaseEpsilon = 1e-8

[Newton]
MaxSteps = 10
TargetSteps = 8
MaxRelativeShift = 1e-5

[Vtk]
AddVelocity = true
