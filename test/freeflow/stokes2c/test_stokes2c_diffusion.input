[TimeManager]
DtInitial = 0.1 # [s]
TEnd = 2000 # [s]

[Grid]
LowerLeft = 0 0 # coordinates of the lower left corner of the domain
UpperRight = 1.0 1.0 # coordinates of the upper right corner of the domain
Cells = 32 32 # number of cells in x-/y-direction

[Stokes]
StabilizationAlpha = -1.0e3 # a stabilization factor. Set to zero for no stabilization

[Problem]
EnableGravity = 0 # disable gravity
Velocity = 0.0 # [m/s]
Name = "stokes2c_diffusion" # prefix of the vtk files
